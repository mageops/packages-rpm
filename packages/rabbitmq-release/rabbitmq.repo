[rabbitmq-server]
name=RabbitMQ Server Official Repository
baseurl=https://packagecloud.io/rabbitmq/rabbitmq-server/el/7/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-RABBITMQ-SERVER
protect=1
repo_gpgcheck=1

[rabbitmq-erlang]
name=RabbitMQ Erlang Official Repository
baseurl=https://packagecloud.io/rabbitmq/erlang/el/7/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-RABBITMQ-ERLANG
protect=1
repo_gpgcheck=1
